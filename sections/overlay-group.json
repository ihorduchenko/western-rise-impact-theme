{
  "type": "custom.overlay",
  "name": "Overlay group",
  "sections": {
    "newsletter-popup": {
      "type": "newsletter-popup",
      "disabled": true,
      "settings": {
        "apparition_delay": 5,
        "show_only_on_index": true,
        "show_only_for_visitors": true,
        "show_only_once": true,
        "title": "Signup for our newsletter",
        "button_text": "Subscribe",
        "subtext": "Describe what your customers will receive when subscribing to your newsletter."
      }
    },
    "privacy-banner": {
      "type": "privacy-banner",
      "disabled": true,
      "custom_css": [],
      "settings": {
        "privacy_bar_title": "Cookie policy",
        "privacy_bar_content": "<p>We use cookies and similar technologies to provide the best experience on our website. Refer to our Privacy Policy for more information.<\/p>"
      }
    },
    "cart-drawer": {
      "type": "cart-drawer",
      "custom_css": [
        ".cart-drawer {--text-color: var(--text-dark);}"
      ],
      "settings": {
        "show_cart_note": false,
        "show_shipping_text": true,
        "show_view_cart_button": false,
        "show_checkout_button": true,
        "widget_code": "<div class=\"rebuy-upsells\"><div data-rebuy-id=\"22092\"><\/div><\/div>",
        "recommendations_title": "",
        "products": [],
        "product_card_background": "#f7f4f0",
        "product_card_text_color": "#222222"
      }
    },
    "search-drawer": {
      "type": "search-drawer",
      "custom_css": [],
      "settings": {
        "full_width": false,
        "show_article_category": false,
        "search_menu": "main-menu",
        "search_menu_show_title": false
      }
    }
  },
  "order": [
    "newsletter-popup",
    "privacy-banner",
    "cart-drawer",
    "search-drawer"
  ]
}