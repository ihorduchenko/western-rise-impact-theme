      {%- endfor -%}
    </div>
  {%- endif -%}

  {%- if collection.metafields.custom.available_variants_part_3 != blank -%}
    <div id="Variants-Part-3">
      <strong class="fw-500">Variants Part-3 (no quotes)</strong><br>
      {%- for variant in collection.metafields.custom.available_variants_part_3.value -%}
        {{ variant.id }},
      {%- endfor -%}
    </div>
  {%- endif -%}

  {%- if collection.metafields.custom.available_variants_part_4 != blank -%}
    <div id="Variants-Part-4">
      <strong class="fw-500">Variants Part-4 (no quotes)</strong><br>
      {%- for variant in collection.metafields.custom.available_variants_part_4.value -%}
        {{ variant.id }},
      {%- endfor -%}
    </div>
  {%- endif -%}

  {%- if collection.metafields.custom.available_variants_part_5 != blank -%}
    <div id="Variants-Part-5">
      <strong class="fw-500">Variants Part-5 (no quotes)</strong><br>
