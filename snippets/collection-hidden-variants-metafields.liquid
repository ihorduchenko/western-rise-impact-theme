      <strong class="fw-500">Variants Part-6 (no quotes)</strong><br>
      {%- for variant in collection.metafields.custom.available_variants_part_6.value -%}
        {{ variant.id }},
      {%- endfor -%}
    </div>
  {%- endif -%}

  <div id="Variants-ALL">
    <strong class="fw-500">Variants (ALL)</strong><br>
    {% for product in collection.products %}
      {% for variant in product.variants %}
        {{ variant.id }},
      {% endfor %}
    {% endfor %}
  </div>
</div>