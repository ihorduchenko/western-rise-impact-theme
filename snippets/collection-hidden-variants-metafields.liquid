<div hidden="hidden" style="word-break: break-all;">
  {%- if collection.metafields.custom.available_variants_part_1 != blank -%}
    <div id="Variants-Part-1">
      <strong class="fw-500">Variants Part-1 (no quotes)</strong><br>
      {%- for variant in collection.metafields.custom.available_variants_part_1.value -%}
        {{ variant.id }},
      {%- endfor -%}
    </div>
  {%- endif -%}

  {%- if collection.metafields.custom.available_variants_part_2 != blank -%}
    <div id="Variants-Part-2">
      <strong class="fw-500">Variants Part-2 (no quotes)</strong><br>
      {%- for variant in collection.metafields.custom.available_variants_part_2.value -%}
